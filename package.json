{
  "name": "puppet-print",
  "version": "1.0.0",
  "description": "A simple Node microservice that converts html to pdf by using Puppeteer",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "compile": "tsc && node dist/server.js",
    "clean": "rm -rf dist && rm -rf node_modules",
    "debug": "./node_modules/nodemon/bin/nodemon.js -e ts  --exec \"npm run compile\"",
    "docker-debug": "docker-compose up",
    "start": "node dist/server.js",
    "tsc": "tsc"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.18.2",
    "express": "4.16.3",
    "puppeteer": "1.16.0"
  },
  "devDependencies": {
    "@types/express": "~4.16.1",
    "@types/node": "~12.0.2",
    "@types/puppeteer": "~1.12.4",
    "nodemon": "^2.0.2",
    "typescript": "~3.4.5"
  }
}
